= form_for @device_position, :html => { :class => 'form-horizontal' } do |f|
  .control-group
    = f.label :device_id, :class => 'control-label'
    .controls
      = f.text_field :device_id, :class => 'text_field'
  .control-group
    = f.label :lat, :class => 'control-label'
    .controls
      = f.text_field :lat, :class => 'text_field'
  .control-group
    = f.label :lng, :class => 'control-label'
    .controls
      = f.text_field :lng, :class => 'text_field'
  .control-group
    = f.label :route_id, :class => 'control-label'
    .controls
      = f.text_field :route_id, :class => 'text_field'
  .form-actions
    = f.submit nil, :class => 'btn btn-primary'
    -#= link_to t('.cancel', :default => t("helpers.links.cancel")), device_position_path, :class => 'btn'
